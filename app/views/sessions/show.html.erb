<body class="loading" id="squad">
  <div id="wrapper">
    <div id="bg"></div>
    <div id="overlay"></div>
    <div id="main">
      <header id="header">
        <h1>Squad</h1>
        <p>Hack all day &nbsp;&bull;&nbsp; Win Gifts &nbsp;&bull;&nbsp; More projects</p>
        <%= link_to "Login With Google", user_omniauth_authorize_path(:google_oauth2), class: "btn-large waves-effect waves-light teal lighten-1"%>
        <p> OR </p>

        <form action= "/search" method="post" style="display: block;"/>
        <select>
          <option>Select City</option>
          <option value="San Francisco">San Francisco</option>
          <option value="Seattle">Seattle</option>
          <option value="Chicago">Chicago</option>
          <option value="New York">New York</option>
        </select>
        <input type="button" value="submit" class="btn-large waves-effect waves-light teal lighten-1">
      </form>
    </header>
  </div>
</div>
</body>